;AAAC3E,YAAS;AAAA,WAAIA,QAAJ;AAAA;AAAV,CAAd;AAAuC,IAAIvC,MAAJ;AAAWJ,OAAOC,KAAP,CAAaC,QAAQ,eAAR,CAAb,EAAsC;AAACE,QAAD,YAAQD,CAAR,EAAU;AAACC,aAAOD,CAAP;AAAS;AAApB,CAAtC,EAA4D,CAA5D;AAA+D,IAAIuH,KAAJ;AAAU1H,OAAOC,KAAP,CAAaC,QAAQ,cAAR,CAAb,EAAqC;AAACwH,OAAD,YAAOvH,CAAP,EAAS;AAACuH,YAAMvH,CAAN;AAAQ;AAAlB,CAArC,EAAyD,CAAzD;AAA4D,IAAIwH,KAAJ;AAAU3H,OAAOC,KAAP,CAAaC,QAAQ,cAAR,CAAb,EAAqC;AAACyH,OAAD,YAAOxH,CAAP,EAAS;AAACwH,YAAMxH,CAAN;AAAQ;AAAlB,CAArC,EAAyD,CAAzD;AAI1L,IAAMwC,WAAW,IAAI+E,MAAME,UAAV,CAAqB,UAArB,CAAjB;;AAEP,IAAIxH,OAAOyH,QAAX,EAAqB;AACjB;AACAzH,SAAO0H,OAAP,CAAe,UAAf;AAA2B,aAASkB,mBAAT,GAA+B;AACtD;AAEA,UAAI,CAAC,KAAKhG,MAAV,EAAkB;AACd,eAAO,KAAKiG,KAAL,EAAP;AACD;;AAED,aAAOtG,SAASgB,IAAT,CAAc;AACnBX,gBAAQ,KAAKA;AADM,OAAd,EAEJ;AACDkG,gBAAQvG,SAASwG;AADhB,OAFI,CAAP;AAKL;;AAZD,WAAoCH,mBAApC;AAAA;AAaH;;AAED5I,OAAO4H,OAAP,CAAe;AACX,mBADW,cACS;AAChB,QAAIoB,MAAMzG,SAASgB,IAAT,CAAc,EAAd,EAAkBE,KAAlB,EAAV;AACA9B,YAAQC,GAAR,CAAY,iBAAZ;AACAD,YAAQC,GAAR,CAAYoH,GAAZ,EAHgB,CAIhB;;AACA,WAAOA,GAAP;AACH,GAPU;AAQX,oBARW,YAQQpG,MARR,EAQgB;AACvB2E,UAAM3E,MAAN,EAAcqG,MAAd;AAEA,QAAI/E,MAAM3B,SAASgB,IAAT,CACN;AAAEX,cAAQA;AAAV,KADM,EAERa,KAFQ,EAAV;AAGA,WAAOS,GAAP;AACH;AAfU,CAAf,E","file":"/app.js","sourcesContent":["import React from \"react\";\nimport {Meteor} from \"meteor/meteor\";\nimport {render} from \"react-dom\";\n\nimport {renderRoutes} from \"../imports/ui/MainManage/Routes.jsx\";\n\nMeteor.startup(() => {\n  render(renderRoutes(), \n  document.getElementById(\"render-target\"));\n});","import React, { Component } from 'react'\r\nimport { withHistory, Link } from 'react-router-dom'\r\nimport { createContainer } from 'meteor/react-meteor-data'\r\n\r\nclass Login extends Component {\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = {\r\n            error: ''\r\n        };\r\n        this.handleSubmit = this.handleSubmit.bind(this);\r\n    }\r\n\r\n    handleSubmit(e) {\r\n        e.preventDefault();\r\n        let email = document.getElementById('login-email').value;\r\n        let password = document.getElementById('login-password').value;\r\n        Meteor.loginWithPassword(email, password, (err) => {\r\n            if (err) {\r\n                this.setState({\r\n                    error: err.reason\r\n                });\r\n            } else {\r\n                this.props.history.push('/');\r\n            }\r\n        });\r\n    }\r\n\r\n    render() {\r\n        const error = this.state.error;\r\n        console.log(error);\r\n\r\n        return (\r\n            <div className=\"\">\r\n                <div className=\"alert alert-primary\" role=\"alert\">\r\n                    This is a primary alertcheck it out!\r\n</div>\r\n                <div className=\"modal-dialog\">\r\n                    <div className=\"modal-content\">\r\n                        <div className=\"modal-header\">\r\n                            <h1 className=\"text-center\">Login</h1>\r\n                        </div>\r\n                        <div className=\"modal-body\">\r\n                            {error.length > 0 ?\r\n                                <div className=\"alert alert-danger\">{error}</div>\r\n                                : ''}\r\n                            <form id=\"login-form\"\r\n                                className=\"form col-md-12 center-block\"\r\n                                onSubmit={this.handleSubmit}>\r\n                                <div className=\"form-group\">\r\n                                    <input type=\"email\"\r\n                                        id=\"login-email\"\r\n                                        className=\"form-control input-lg\"\r\n                                        placeholder=\"Email\" />\r\n                                </div>\r\n                                <div className=\"form-group\">\r\n                                    <input type=\"password\"\r\n                                        id=\"login-password\"\r\n                                        className=\"form-control input-lg\"\r\n                                        placeholder=\"Password\" />\r\n                                </div>\r\n                                <div className=\"form-group text-center\">\r\n                                    <input type=\"submit\"\r\n                                        id=\"login-button\"\r\n                                        className=\"btn btn-primary btn-lg btn-block\"\r\n                                        value=\"Login\" />\r\n                                </div>\r\n                                <div className=\"form-group text-center\">\r\n                                    <p className=\"text-center\">\r\n                                        Don't have an account? Register <Link to=\"/signup\">here</Link>\r\n                                    </p>\r\n                                </div>\r\n                            </form>\r\n                        </div>\r\n                        <div className=\"modal-footer\" style={{ borderTop: 0 }}></div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default Login;","import React, { Component } from 'react';\r\nimport { withHistory, Link } from 'react-router-dom';\r\nimport { Accounts } from 'meteor/accounts-base';\r\n\r\nclass Signup extends Component {\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = {\r\n            error: ''\r\n        };\r\n        this.handleSubmit = this.handleSubmit.bind(this);\r\n    }\r\n\r\n    handleSubmit(e) {\r\n        e.preventDefault();\r\n        let name = document.getElementById(\"signup-name\").value;\r\n        let email = document.getElementById(\"signup-email\").value;\r\n        let password = document.getElementById(\"signup-password\").value;\r\n        this.setState({ error: \"test\" });\r\n        Accounts.createUser({ email: email, username: name, password: password }, (err) => {\r\n            if (err) {\r\n                this.setState({\r\n                    error: err.reason\r\n                });\r\n            } else {\r\n                this.props.history.push('/login');\r\n            }\r\n        });\r\n    }\r\n\r\n    render() {\r\n        const error = this.state.error;\r\n        return (\r\n            <div className=\"\">\r\n                <div className=\"modal-dialog\">\r\n                    <div className=\"modal-content\">\r\n                        <div className=\"modal-header\">\r\n                            <h1 className=\"text-center\">Sign up</h1>\r\n                        </div>\r\n                        <div className=\"modal-body\">\r\n                            {error.length > 0 ?\r\n                                <div className=\"alert alert-danger fade in\">{error}</div>\r\n                                : ''}\r\n                            <form id=\"login-form\"\r\n                                className=\"form col-md-12 center-block\"\r\n                                onSubmit={this.handleSubmit}>\r\n                                <div className=\"form-group\">\r\n                                    <input type=\"text\" id=\"signup-name\"\r\n                                        className=\"form-control input-lg\" placeholder=\"Name\" />\r\n                                </div>\r\n                                <div className=\"form-group\">\r\n                                    <input type=\"email\" id=\"signup-email\"\r\n                                        className=\"form-control input-lg\" placeholder=\"Email\" />\r\n                                </div>\r\n                                <div className=\"form-group\">\r\n                                    <input type=\"password\" id=\"signup-password\"\r\n                                        className=\"form-control input-lg\"\r\n                                        placeholder=\"Password\" />\r\n                                </div>\r\n                                <div className=\"form-group\">\r\n                                    <input type=\"submit\" id=\"login-button\"\r\n                                        className=\"btn btn-lg btn-primary btn-block\"\r\n                                        value=\"Sign Up\" />\r\n                                </div>\r\n                                <div className=\"form-group\">\r\n                                    <p className=\"text-center\">\r\n                                        Already have an account? Login <Link to=\"/login\">here</Link>\r\n                                    </p>\r\n                                </div>\r\n                            </form>\r\n                        </div>\r\n                        <div className=\"modal-footer\" style={{ borderTop: 0 }}></div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default Signup;","import React, { Component } from 'react';\r\nimport { withHistory } from 'react-router-dom';\r\nimport { withTracker } from \"meteor/react-meteor-data\";\r\n\r\nimport MainPage from './MainPage.jsx';\r\n\r\nimport { UserData } from \"../../api/UserData\";\r\n\r\nclass App extends Component {\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = this.getMeteorData();\r\n        this.logout = this.logout.bind(this);\r\n    }\r\n\r\n    getMeteorData() {\r\n        return { isAuthenticated: Meteor.userId() !== null };\r\n    }\r\n\r\n    componentWillMount() {\r\n        if (!this.state.isAuthenticated) {\r\n            this.props.history.push('/login');\r\n        }\r\n    }\r\n\r\n    componentDidUpdate(prevProps, prevState) {\r\n        if (!this.state.isAuthenticated) {\r\n            this.props.history.push('/login');\r\n        }\r\n    }\r\n\r\n    logout(e) {\r\n        e.preventDefault();\r\n        Meteor.logout((err) => {\r\n            if (err) {\r\n                console.log(err.reason);\r\n            } else {\r\n                this.props.history.push('/login');\r\n            }\r\n        });\r\n    }\r\n\r\n    render() {\r\n\r\n        return (\r\n            <div id=\"App\">\r\n                {this.props.user && this.props.userData ? <MainPage logout={this.logout} user={this.props.user} userData={this.props.userData[0]}/> : \"Loading user settings...\"}\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nApp.propTypes = {\r\n\r\n};\r\n\r\nexport default withTracker(\r\n    () => {\r\n        Meteor.subscribe('users');\r\n        Meteor.subscribe('UserData');\r\n\r\n        const currentUser = Meteor.user();\r\n        let currentUserData = null;\r\n\r\n        if(currentUser != null){\r\n            //console.log(\"Searching for data of \" + currentUser._id);\r\n            currentUserData = UserData.find(\r\n                { userId: currentUser._id }\r\n            ).fetch();\r\n        }\r\n\r\n        return {\r\n            user: currentUser,\r\n            userData: currentUserData\r\n        }\r\n    }\r\n)(App);","import React, { Component } from 'react';\r\n\r\nimport \"../css/BasicNav.css\";\r\n\r\nclass BasicNav extends Component {\r\n    render() {\r\n        return (\r\n            <div id=\"BasicNav\" className=\"myNav\">\r\n                <nav className=\"navbar navbar-expand-lg navbar-dark bg-dark myNav\">\r\n                    <a className=\"navbar-brand col-sm-3 col-md-2 mr-0\" href=\"#\">\r\n                        <img src=\"\" width=\"30\" height=\"30\" className=\"d-inline-block align-top\" alt=\"\" />\r\n                        Bet Simulator\r\n                    </a>\r\n\r\n                    <button className=\"navbar-toggler\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navContent\" aria-controls=\"navContent\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\r\n                        <span className=\"navbar-toggler-icon\"></span>\r\n                    </button>\r\n\r\n                    <div className=\"collapse navbar-collapse\" id=\"navContent\">\r\n                    \r\n                        <ul className=\"navbar-nav mr-auto mt-2 mt-lg-0\">\r\n                            <li className=\"nav-item active\">\r\n                                <a name=\"\" className=\"nav-link\">Welcome {this.props.userName}! <span className=\"sr-only\">(current)</span></a>\r\n                            </li>\r\n\r\n                            <li className=\"nav-item\">\r\n                                <a id=\"coins-info\" className=\"nav-link\">Coins bag: {this.props.coins}</a>\r\n                            </li>\r\n\r\n                            <li className=\"nav-item\">\r\n                                <a id=\"coins-info\" className=\"nav-link\">In bet: 0</a>\r\n                            </li>\r\n                        </ul>\r\n\r\n                        <button onClick={this.props.logout} className=\"btn btn-outline-danger my-2 my-sm-0\" type=\"submit\">Log out</button>\r\n                    </div>\r\n                </nav>\r\n\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default BasicNav;","import React, { Component } from 'react';\r\nimport PropTypes from 'prop-types';\r\n\r\nimport \"../css/BetCard.css\";\r\n\r\nclass BetCard extends Component {\r\n    constructor(props) {\r\n        super(props);\r\n\r\n        this.state = {\r\n            colSize: \"col-md-4\"\r\n        };\r\n    }\r\n\r\n    getFee(percent) {\r\n        let res = 100 / percent;\r\n\r\n        return Math.round(res * 100) / 100;\r\n    }\r\n\r\n    componentDidMount() {\r\n        if (this.state.colSize) {\r\n            window.addEventListener('resize', () => {\r\n                let nColSize = \"col\"\r\n\r\n                let width = window.innerWidth;\r\n\r\n                if (width >= 1500) {\r\n                    nColSize = \"col-md-3\";\r\n                } else if (width > 900 && width < 1500) {\r\n                    nColSize = \"col-md-4\";\r\n                } else {\r\n                    nColSize = \"col-md-6\";\r\n                }\r\n\r\n                this.setState({ colSize: nColSize });\r\n            }, false);\r\n        }\r\n    }\r\n\r\n    render() {\r\n        const txtLoading = \"Loading...\";\r\n        let Name = txtLoading;\r\n        let Place = txtLoading;\r\n        let Image = \"Basic_Event.png\";\r\n        let Team1 = txtLoading;\r\n        let Team2 = txtLoading;\r\n        let Prob1 = txtLoading;\r\n        let Tie = txtLoading;\r\n        let Prob2 = txtLoading;\r\n\r\n        let bInfo = this.props.betInfo;\r\n        let eInfo = this.props.eventInfo;\r\n\r\n        if (eInfo != null && eInfo != undefined && eInfo.length > 0) {\r\n            eInfo = eInfo[0];\r\n            \r\n            console.log(eInfo);\r\n\r\n            Name = eInfo.Name;\r\n            Place = eInfo.Place;\r\n            Image = eInfo.Image;\r\n            Team1 = eInfo.Team1;\r\n            Team2 = eInfo.Team2;\r\n            Prob1 = eInfo.Prob1;\r\n            Tie = eInfo.Tie;\r\n            Prob2 = eInfo.Prob2;\r\n\r\n        }\r\n\r\n        return (\r\n            <div id={Name} className={this.state.colSize + \" myBetCard\"}>\r\n                <div className=\"card myCard\">\r\n                    <img className=\"card-img-top\" src={\"img/\" + Image} alt=\"Card image cap\" />\r\n                    <div className=\"card-body\">\r\n                        <h4 className=\"card-title\">{Name}</h4>\r\n                        <h5 className=\"card-title\">{Place}</h5>\r\n\r\n                        <div className=\"btn-group myButtonGroup\">\r\n                            <span className=\"badge badge-primary myButtonGroup\">{Team1}</span>\r\n                            <span className=\"badge badge-secondary myButtonGroup\">Tie</span>\r\n                            <span className=\"badge badge-primary myButtonGroup\">{Team2}</span>\r\n                        </div>\r\n\r\n                        <div className=\"btn-group myButtonGroup\" role=\"group\" aria-label=\"Basic example\">\r\n                            <button type=\"button\" className=\"btn btn-primary myButtonOnGroup\">{\"\" + this.getFee(Prob1)}</button>\r\n                            <button type=\"button\" className=\"btn btn-secondary myButtonOnGroup\">{\"\" + this.getFee(Tie)}</button>\r\n                            <button type=\"button\" className=\"btn btn-primary myButtonOnGroup\">{\"\" + this.getFee(Prob2)}</button>\r\n                        </div>\r\n\r\n                        <hr className=\"my-4\" />\r\n                        <h5 className=\"card-title\">Bets...</h5>\r\n\r\n                        <div className=\"btn-group myButtonGroup\" role=\"group\" aria-label=\"Basic example\">\r\n                            <button type=\"button\" className=\"btn btn-warning myButtonOnGroup\">{bInfo.Team1}</button>\r\n                            <button type=\"button\" className=\"btn btn-warning myButtonOnGroup\">{bInfo.Tie}</button>\r\n                            <button type=\"button\" className=\"btn btn-warning myButtonOnGroup\">{bInfo.Team2}</button>\r\n                        </div>\r\n\r\n                        <hr className=\"my-4\" />\r\n\r\n                        <button className=\"btn btn-outline-info myCardButton\" type=\"submit\">More info</button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nBetCard.propTypes = {\r\n    betInfo: PropTypes.object.isRequired,\r\n    eventInfo: PropTypes.array.isRequired\r\n}\r\n\r\nexport default BetCard;","import React, { Component } from 'react';\r\nimport { withHistory, Link } from 'react-router-dom';\r\nimport PropTypes from 'prop-types';\r\nimport { withTracker } from \"meteor/react-meteor-data\";\r\n\r\nimport BasicNav from \"./BasicNav.jsx\";\r\nimport CategoryPage from \"./CategoryPage.jsx\";\r\nimport MyBets from \"./MyBets.jsx\";\r\n\r\nimport { Categories } from \"../../api/Categories\";\r\nimport { Events } from \"../../api/Events\";\r\nimport { UserData } from \"../../api/UserData\";\r\n\r\nimport \"../css/MainPage.css\";\r\n\r\nclass MainPage extends Component {\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = {\r\n            username: ''\r\n        };\r\n    }\r\n\r\n    componentDidMount() {\r\n    }\r\n\r\n    loadCategoriesList() {\r\n        let actCategories = this.props.categories;\r\n        let res = [];\r\n\r\n        res.push(\r\n            <li key=\"MyBetsOption\" className=\"nav-item\">\r\n                <a href={\"#MyBets\"} className=\"nav-link\" >\r\n                    My Bets...\r\n                </a>\r\n            </li>\r\n        );\r\n\r\n        res.push(actCategories.map(e => (\r\n            <li key={e.name} className=\"nav-item\">\r\n                <a href={\"#\" + e.name} className=\"nav-link\" >\r\n                    {e.name}\r\n                </a>\r\n            </li>\r\n        )));\r\n\r\n        return res;\r\n    }\r\n\r\n    loadCategoryPages() {\r\n        let actCategories = this.props.categories;\r\n        let res = [];\r\n\r\n        //console.log(this.props.userData);\r\n        \r\n        res.push(<MyBets key=\"MyBetsPage\" myBets={this.props.userData.Bets} />);\r\n\r\n        res.push( actCategories.map(e => (\r\n            <CategoryPage key={e.name + \"Page\"} categoryInfo={e} events={Events.find({ Category: e.name }).fetch()} userData={this.props.userData} />\r\n        )));\r\n\r\n        return res;\r\n    }\r\n\r\n    render() {\r\n        let currentUser = this.props.user;\r\n        let currentUserAvailable = (currentUser !== undefined);\r\n\r\n        let currentUserData = this.props.userData;\r\n        let userDataAvailable = (currentUserData !== undefined);\r\n\r\n        let loggedIn = (currentUser && currentUserAvailable && currentUserData && userDataAvailable);\r\n\r\n        let numCoins = 0;\r\n\r\n        if (loggedIn) {\r\n            /*console.log(\"Render | loggedIn | AllDta: \");\r\n            console.log(currentUser);\r\n            console.log(currentUserData);*/\r\n\r\n            numCoins = currentUserData.coins;\r\n\r\n            let actCategories = this.props.categories;\r\n\r\n            if (actCategories != null && actCategories != undefined && actCategories.length > 0) {\r\n                //console.log(actCategories);\r\n            }\r\n        }\r\n\r\n        return (\r\n            <div id=\"MainPage\">\r\n                {loggedIn ? <BasicNav logout={this.props.logout} userName={currentUser.username} coins={numCoins} /> : \"Error\"}\r\n\r\n                <div className=\"container-fluid\">\r\n                    <div className=\"row\">\r\n\r\n                        <nav className=\"col-md-2 bg-light sidebar\">\r\n                            <div className=\"sidebar-sticky\">\r\n                                <ul className=\"nav flex-column\">\r\n                                    {this.loadCategoriesList()}\r\n                                </ul>\r\n                            </div>\r\n                        </nav>\r\n\r\n                        <main role=\"main\" className=\"col-md-10 categoryPageContainer\">\r\n                            {this.loadCategoryPages()}\r\n                        </main>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nMainPage.propTypes = {\r\n    user: PropTypes.object.isRequired,\r\n    userData: PropTypes.object.isRequired\r\n}\r\n\r\nexport default withTracker(\r\n    () => {\r\n        Meteor.subscribe(\"Categories\");\r\n        Meteor.subscribe(\"Events\");\r\n        Meteor.subscribe(\"UserData\");\r\n\r\n        return {\r\n            categories: Categories.find({}, { sort: { name: 1 } }).fetch(),\r\n            events: Events.find({}).fetch()\r\n        }\r\n    }\r\n)(MainPage);","import React, { Component } from 'react';\r\nimport PropTypes from 'prop-types';\r\nimport { withTracker } from \"meteor/react-meteor-data\";\r\n\r\nimport BetCard from \"./BetCard.jsx\";\r\nimport { Events } from \"../../api/Events\";\r\nimport \"../css/MyBets.css\";\r\n\r\nclass MyBets extends Component {\r\n\r\n    loadBetsList() {\r\n        let actBets = this.props.myBets;\r\n        let res = \"\";\r\n\r\n        //console.log(this.props.categoryInfo);\r\n\r\n        res = actBets.map(e => (\r\n            //console.log(\"MyBets | loadBetsList | Bet: \" + e),\r\n            //console.log(e),\r\n            <BetCard key={e.Name + \"Card\"} betInfo={e} eventInfo={Events.find( { _id: e.EventId }, { limit: 1} ).fetch()} />\r\n        ));\r\n\r\n        return res;\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div id=\"MyBets\" className=\"container myEvents\">\r\n                <h1>My Bets</h1>\r\n\r\n                <hr className=\"my-4\" />\r\n\r\n                <div className=\"row\">\r\n                    {this.loadBetsList()}\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nMyBets.propTypes = {\r\n    myBets: PropTypes.array.isRequired\r\n}\r\n\r\n\r\nexport default withTracker(\r\n    () => {\r\n        Meteor.subscribe(\"Events\");\r\n\r\n        return {\r\n            events: Events.find({}).fetch()\r\n        }\r\n    }\r\n)(MyBets);","import React from 'react'\r\nimport { BrowserRouter as Router, Route } from 'react-router-dom';\r\n\r\n// Bases\r\nimport App from './App.jsx'\r\n\r\n// Pages\r\nimport Login from '../Accounts/Login.jsx'\r\nimport Signup from '../Accounts/Signup.jsx'\r\n\r\nexport const renderRoutes = () => (\r\n  <Router>\r\n    <div id=\"Routes\">\r\n      <Route exact={true} path=\"/\" component={App}/>\r\n\r\n      <Route path=\"/login\" component={Login}/>\r\n      <Route path=\"/signup\" component={Signup}/>\r\n    </div>\r\n  </Router>\r\n);","import { Meteor } from \"meteor/meteor\";\r\nimport { Mongo } from \"meteor/mongo\";\r\nimport { check } from \"meteor/check\";\r\n\r\nexport const Categories = new Mongo.Collection(\"Categories\");\r\n\r\nif (Meteor.isServer) {\r\n    // This code only runs on the server\r\n    Meteor.publish('Categories', function CategoriesPublication() {\r\n        return Categories.find();\r\n/*\r\n        if (!this.userId) {\r\n            return this.ready();\r\n          }\r\n        \r\n          return UserData.find({\r\n            userId: this.userId\r\n          }, {\r\n            fields: UserData.publicFields\r\n          });*/\r\n    });\r\n}\r\n\r\nMeteor.methods({\r\n  \"Categories.getAll\"(){\r\n    let res = Categories.find({}).fetch();\r\n    console.log(res);\r\n  }\r\n\r\n});","import { Meteor } from \"meteor/meteor\";\r\nimport { Mongo } from \"meteor/mongo\";\r\nimport { check } from \"meteor/check\";\r\n\r\nexport const Events = new Mongo.Collection(\"Events\");\r\n\r\nif (Meteor.isServer) {\r\n    // This code only runs on the server\r\n    Meteor.publish('Events', function EventsPublication() {\r\n        return Events.find();\r\n        /*\r\n                if (!this.userId) {\r\n                    return this.ready();\r\n                  }\r\n                \r\n                  return UserData.find({\r\n                    userId: this.userId\r\n                  }, {\r\n                    fields: UserData.publicFields\r\n                  });*/\r\n    });\r\n}\r\n\r\nMeteor.methods({\r\n    \"Events.getAll\"() {\r\n        let res = Events.find({}).fetch();\r\n        console.log(res);\r\n    },\r\n    \"Events.addEvent\"(category, name, place, date, image, team1, team2, prob1, prob2, tie){\r\n        let txtTest = \"\";\r\n\r\n        txtTest += (category + \"\\n\");\r\n        txtTest += (name + \"\\n\");\r\n        txtTest += (place + \"\\n\");\r\n        txtTest += (date + \"\\n\");\r\n        txtTest += (image + \"\\n\");\r\n        txtTest += (team1 + \"\\n\");\r\n        txtTest += (team2 + \"\\n\");\r\n        txtTest += (prob1 + \"\\n\");\r\n        txtTest += (prob2 + \"\\n\");\r\n        txtTest += (tie + \"\\n\");\r\n\r\n        console.log(txtTest);\r\n\r\n        let nEvent = {\r\n            Category: category,\r\n            Name: name,\r\n            Place: place,\r\n            Imagen: image,\r\n            Team1: team1,\r\n            Team2: team2,\r\n            Date: date,\r\n            Prob1: prob1,\r\n            Prob2: prob2,\r\n            Tie: tie,\r\n            Bets: []\r\n        };\r\n\r\n        Events.insert(nEvent);\r\n    }\r\n});","import { Meteor } from \"meteor/meteor\";\r\nimport { Mongo } from \"meteor/mongo\";\r\nimport { check } from \"meteor/check\";\r\n\r\nexport const UserData = new Mongo.Collection(\"UserData\");\r\n\r\nif (Meteor.isServer) {\r\n    // This code only runs on the server\r\n    Meteor.publish('UserData', function userDataPublication() {\r\n        //return UserData.find();\r\n\r\n        if (!this.userId) {\r\n            return this.ready();\r\n          }\r\n        \r\n          return UserData.find({\r\n            userId: this.userId\r\n          }, {\r\n            fields: UserData.publicFields\r\n          });\r\n    });\r\n}\r\n\r\nMeteor.methods({\r\n    \"UserData.getAll\"() {\r\n        let all = UserData.find({}).fetch();\r\n        console.log(\"All user data: \");\r\n        console.log(all);\r\n        //Lo Anterior funciona\r\n        return all;\r\n    },\r\n    \"UserData.getData\"(userId) {\r\n        check(userId, String);\r\n\r\n        let res = UserData.find(\r\n            { userId: userId }           \r\n        ).fetch();\r\n        return res;\r\n    }\r\n});\r\n"]}}]