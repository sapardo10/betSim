\n\r\n            if (actEventsInfo != null && actEventsInfo != undefined && actEventsInfo.length > 0) {\r\n                //console.log(actEventsInfo);\r\n            }\r\n        }\r\n\r\n        return (\r\n            <div>\r\n                {loggedIn ? <BasicNav logout={this.props.logout} userName={currentUser.username} coins={numCoins} /> : \"Error\"}\r\n\r\n                <div className=\"container-fluid\">\r\n                    <div className=\"row\">\r\n                        <nav className=\"col-md-2 d-none d-md-block bg-light sidebar\">\r\n                            <div className=\"sidebar-sticky\">\r\n                                <ul className=\"nav flex-column\">\r\n                                    {this.loadEventsList()}\r\n                                </ul>\r\n                            </div>\r\n                        </nav>\r\n\r\n                        <main role=\"main\" className=\"col-md-9 ml-sm-auto col-lg-10 pt-3 px-4\">\r\n                            {this.loadEventsPages()}\r\n                        </main>\r\n                    </div>\r\n\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nMainPage.propTypes = {\r\n    user: PropTypes.object.isRequired,\r\n    userData: PropTypes.object.isRequired\r\n}\r\n\r\nexport default withTracker(\r\n    () => {\r\n        Meteor.subscribe(\"EventsInfo\");\r\n\r\n        return {\r\n            eventsInfo: EventsInfo.find({}).fetch()\r\n        }\r\n    }\r\n)(MainPage);","import React from 'react'\r\nimport { BrowserRouter as Router, Route } from 'react-router-dom';\r\n\r\n// Bases\r\nimport App from './App.jsx'\r\n\r\n// Pages\r\nimport Login from '../Accounts/Login.jsx'\r\nimport Signup from '../Accounts/Signup.jsx'\r\n\r\nexport const renderRoutes = () => (\r\n  <Router>\r\n    <div>\r\n      <Route exact={true} path=\"/\" component={App}/>\r\n\r\n      <Route path=\"/login\" component={Login}/>\r\n      <Route path=\"/signup\" component={Signup}